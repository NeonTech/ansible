# Target DNS server (IPv4 or IPv6 address, not a hostname)
dns_rfc2136_server = {{ redis_server_tls_acme_rfc2136_ip }}
# Target DNS port
dns_rfc2136_port = {{ redis_server_tls_acme_rfc2136_port }}
# TSIG key name
dns_rfc2136_name = {{ redis_server_tls_acme_rfc2136_name }}
# TSIG key secret
dns_rfc2136_secret = {{ redis_server_tls_acme_rfc2136_secret }}
# TSIG key algorithm
dns_rfc2136_algorithm = {{ redis_server_tls_acme_rfc2136_algorithm }}
